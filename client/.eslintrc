{
  "extends": "airbnb-base",
  "env": {
    "browser": true
  },
"parser": "@typescript-eslint/parser",
"parserOptions": {
    "ecmaFeatures": {
        "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
},
"plugins": [
    "react",
    "@typescript-eslint"
],
  "rules": {
    "linebreak-style": 0,
    "global-require": 0,
    "eslint linebreak0style": [0, "error, windows"]
  }
}